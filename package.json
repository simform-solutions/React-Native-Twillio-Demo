{
  "name": "TwilioVideoCall",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "lint": "eslint ./App",
    "fixcode": "eslint --fix ./App",
    "ios": "npm run lint && react-native run-ios",
    "android": "npm run lint && react-native run-android"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "App/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "native-base": "^2.10.0",
    "prop-types": "^15.6.2",
    "react": "16.6.3",
    "react-native": "0.57.8",
    "react-native-extended-stylesheet": "^0.11.1",
    "react-native-gesture-handler": "^1.0.12",
    "react-native-twilio-video-webrtc": "git+https://github.com/blackuy/react-native-twilio-video-webrtc.git",
    "react-navigation": "^3.0.9"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-jest": "23.6.0",
    "eslint": "^5.11.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.0",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.51.1",
    "react-test-renderer": "16.6.3",
    "eslint-plugin-eslint-comments": "^3.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3"
  },
  "jest": {
    "preset": "react-native"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "rnpm": {
    "assets": [
      "./App/Fonts/"
    ]
  }
}
